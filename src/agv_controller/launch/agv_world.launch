<launch>


<!-- Start Gazebo with an empty world -->
  	<include file="$(find gazebo_ros)/launch/empty_world.launch">
    	<arg name="world_name" value="world"/>
	</include>

	<!-- Load controller parameters  -->
	<rosparam file="$(find agv_controller)/config/agv_params.yaml" command="load"/>
	<!-- Load urdf inside parameter server -->

	<param name="robot_description"
    textfile="$(find agv_controller)/models/model.urdf" />


<!-- Launch model inside gazebo -->


	<node pkg="gazebo_ros" name="urdf_spawner" type="spawn_model" respawn="false" output="screen" args="-urdf -model agv -param robot_description"/>





</launch>